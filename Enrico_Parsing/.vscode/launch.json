{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Minishell",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/Enrico_Parsing/minishell",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/Enrico_Parsing",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "C/C++: gcc build active file",
            "miDebuggerPath": "/usr/bin/gdb"
        }
    ]
}